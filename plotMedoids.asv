function plotMedoids(medoidList, snapshotList)

for i = 1:length(medoidList)
    scatter(medoidList{i}(:,1), medoidList{i}(:,2), 20, [0 0 256]/256, 'filled')
    
    hold on
    for j = 1:length(snapshotList{i})
        scatter(snapshotList{i}{j}.xmm, snapshotList{i}{j}.ymm, 15, [169,169,169]/256)
    end
    
    hold off
    axis equal
    set(gca,'XLim',[-800 -400],'YLim',[500 700])
    pause(0.5)
end

end
